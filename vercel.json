{
  "version": 2,
  "builds": [
    { "src": "api/chat.js", "use": "@vercel/node" },
    { "src": "api/compile.js", "use": "@vercel/node" },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },

    
    { "source": "/(.*)", "destination": "/" }
  ],
  "redirects": [
    
    { "source": "/(.*)", "destination": "https://www.focsera.in/$1", "permanent": true }
  ]
}
